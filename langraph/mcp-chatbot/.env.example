DEBUG=False

# Database (Supabase PostgreSQL)
DATABASE_URL=postgresql://user:password@host:5432/postgres

# Redis (optional)
REDIS_URL=redis://localhost:6379

# OpenAI
OPENAI_API_KEY=sk-xxx

# AWS Bedrock (optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
